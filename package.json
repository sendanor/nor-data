{
  "name": "nor-data",
  "version": "1.0.0",
  "description": "Helpers for dealing with JavaScript data",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run test-spec",
    "test-dot": "mocha -u bdd -c -R dot tests/test-*.js",
    "test-list": "mocha -u bdd -c -R list tests/test-*.js",
    "test-spec": "mocha -u bdd -c -R spec tests/test-*.js",
    "test-progress": "mocha -u bdd -c -R progress tests/test-*.js",
    "test-min": "mocha -u bdd -c -R min tests/test-*.js",
    "test-markdown": "mocha -u bdd -c -R markdown tests/test-*.js",
    "build-tests.md": "npm run test-markdown > tests.md",
    "lint": "node-lint src/ --config=lint.json",
    "jshint": "jshint src/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sendanor/nor-data.git"
  },
  "keywords": [
    "data",
    "json",
    "object",
    "copy"
  ],
  "dependencies": {
    "nor-is"    : "^0.1.18",
    "nor-debug" : "^0.1.55",
    "nor-array" : "^0.1.8"
  },
  "devDependencies": {
    "lint"    : "^1.1.2",
    "mocha"   : "^2.5.3",
    "vows"    : "^0.8.1",
    "jshint"  : "^2.9.2"
  },
  "author": "Jaakko-Heikki Heusala <jheusala@iki.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sendanor/nor-data/issues"
  },
  "homepage": "https://github.com/sendanor/nor-data"
}
